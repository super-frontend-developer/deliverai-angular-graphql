{
  "private": true,
  "name": "deliverai-frontend-web-dashboard",
  "version": "1.0.16",
  "scripts": {
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "prestart": "yarn run graphql:codegen",
    "start": "ng serve",
    "start:prod": "yarn build:prod && http-server dist/application -p 4201 -di --gzip --log-ip",
    "start:proxy:local": "ng serve --proxy-config proxy/local.proxy.conf.json",
    "start:proxy:prod": "ng serve --proxy-config proxy/prod.proxy.conf.json",
    "prebuild": "yarn run graphql:codegen",
    "build": "ng build",
    "prebuild:prod": "yarn run graphql:codegen",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "yarn lint:angular && yarn lint:scss",
    "lint:angular": "ng lint",
    "lint:scss": "stylelint --config .stylelintrc.js --syntax scss \"src/**/**/*.scss\"",
    "audit": "yarn audit",
    "audit:high": "yarn audit --level high",
    "audit:high:exitcode": "yarn audit:high; [[ $? -ge 8 ]] && exit 1 || exit 0",
    "docker": "yarn docker:build && yarn docker:run",
    "docker:build": "docker build -t $npm_package_config_docker_image_repository:$npm_package_config_docker_image_tag .",
    "docker:run": "docker run -it --name $npm_package_config_docker_container_name --rm -p $npm_package_config_docker_container_port:80 $npm_package_config_docker_image_repository:$npm_package_config_docker_image_tag",
    "docker:open": "opener http://localhost:$npm_package_config_docker_container_port",
    "release": "release-it"
  },
  "config": {
    "docker": {
      "image": {
        "repository": "dashboard-application",
        "tag": "latest"
      },
      "container": {
        "name": "dashboard-application",
        "port": 14200
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/elements": "^9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ngneat/until-destroy": "^7.1.6",
    "@ngrx/data": "^9.1.2",
    "@ngrx/effects": "^9.1.2",
    "@ngrx/entity": "^9.1.2",
    "@ngrx/router-store": "^9.1.2",
    "@ngrx/store": "^9.1.2",
    "@ngrx/store-devtools": "^9.1.2",
    "angled-edges": "^2.0.0",
    "angular-oauth2-oidc": "^9.2.2",
    "apollo-angular": "^1.9.1",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-cache-inmemory": "^1.6.6",
