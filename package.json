    "bourbon": "^7.0.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "cross-domain-storage": "^2.0.4",
    "cross-storage": "^1.0.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "ng2-charts": "^2.3.2",
